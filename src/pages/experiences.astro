---

import BaseLayout from '../layouts/BaseLayout.astro'

// page
const title = 'Eric Hazard Mes expériences';
const description = "Découvrez les expériences professionnelles ou de projets personnels dans le développement Web d'Eric Hazard, développeur Web sur Toulouse";
const canonicalUrl = Astro.request.canonicalURL;
const activeLink = "experiences";

